{
  "name": "sci-map-frontend",
  "version": "1.0.0",
  "description": "The frontend ofc",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "dev:ext": "node web_extension/generateManifest.js --dev && parcel web_extension/index.html --out-dir web_extension/build/development",
    "build:ext": "rm -rf web_extension/build/production && node web_extension/generateManifest.js && parcel build web_extension/index.html -d web_extension/build/production --no-source-maps && cd web_extension/build/production && zip -r -FS ../../deploy/SciMap.zip *",
    "dist": "sh scripts/dist.sh",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "schema": "graphql-codegen --config genSchema.yml",
    "docker:build": "docker build -t scimaporg/frontend:latest .",
    "docker:run": "docker run -it -p 3000:3000 --env-file .env.local scimaporg/frontend:latest",
    "docker:push": "docker push scimaporg/frontend:latest",
    "deploy": "yarn docker:build && yarn docker:push && yarn dist && echo 'done, just upload the zip file to beanstalk'",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sci-map-org/frontend.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
